# Flags to compile and link FEMOCS

## release mode optimization flags
FEMOCS_OPT=

## debug mode optimization flags
FEMOCS_DOPT=

## paths to headers
FEMOCS_HEADPATH=
FEMOCS_EXT_HEAD=

## paths to libraries
FEMOCS_LIBPATH=
FEMOCS_EXT_LIB=

## release mode libraries
FEMOCS_LIB=

## debug mode libraries
FEMOCS_DLIB=

## flags for building Tetgen and Deal.II libraries
TETGEN_FLAGS=
DEALII_FLAGS=

## flag indicating in which machine the compilation happens
MACHINE=
